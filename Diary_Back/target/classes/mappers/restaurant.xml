<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mappers.restaurant">
    <select id="getRestaurantTozipcode" resultType="restaurant" parameterType="java.util.HashMap">
            select * from restaurant_Seodaemun WHERE #{zipcodeMin} <![CDATA[<]]> zipcode AND zipcode <![CDATA[<]]> #{zipcodeMax} order by rand() limit 1;
    </select>

    <select id="mappers.restaurant.getRestaurantToname" resultType="java.util.List" parameterType="java.lang.String">
        SELECT * FROM restaurant_Seodaemun  WHERE restaurantName LIKE #{value};
    </select>
</mapper>